{
  "name": "deep-cuts",
  "version": "2.6.1",
  "description": "Useful utilities and rare b-sides.",
  "author": "Trevor Ewen",
  "license": "MIT",
  "module": "dist/deep-cuts.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "homepage": "https://github.com/tewen/deep-cuts#readme",
  "bugs": {
    "url": "https://github.com/tewen/deep-cuts/issues"
  },
  "keywords": [
    "Util",
    "Useful",
    "Helpers"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "test-watch": "tsdx test --watch",
    "lint": "tsdx lint",
    "lint-fix": "tsdx lint --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/deep-cuts.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/deep-cuts.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "qs": "^6.5.2"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.4",
    "@types/jest": "^25.2.3",
    "@types/qs": "^6.9.7",
    "axios": "^0.24.0",
    "data-mining-tools": "^3.0.0",
    "husky": "^7.0.1",
    "size-limit": "^5.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  }
}
